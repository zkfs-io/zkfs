{
  "compilerOptions": {
    "strictNullChecks": true,
    "lib": ["ESNext"],
    "target": "ESNext",
    "module": "ESNext",
    "declaration": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
  }
}
